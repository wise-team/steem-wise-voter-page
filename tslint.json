{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "double"
        ],
        "indent": [
            true
        ],
        "interface-name": [
            false
        ],
        "arrow-parens": false,
        // Pending fix for shorthand property names.
        "object-literal-sort-keys": false,

        "ordered-imports": false,
        "object-literal-shorthand": false,
        "no-consecutive-blank-lines": false,
        "trailing-comma": false,
        "curly": false,
        "prefer-for-of": false
    },
    "rulesDirectory": []
}